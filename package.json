{
  "name": "cadence-web",
  "version": "4.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 8080",
    "build": "NODE_ENV=production next build",
    "start": "next start",
    "lint": "next lint",
    "install": "npm run install-idl",
    "install-idl": "cd node_modules && npx tiged https://github.com/uber/cadence-idl#34b4519b270945fbb38f413137dfb50b5fc20af7 cadence-idl",
    "generate:idl": "mkdir -p src/idl; npm run generate:idl:proto",
    "generate:idl:proto": "rm -rf src/idl/proto && cp -R node_modules/cadence-idl/proto src/idl/proto",
    "postinstall": "npm run generate:idl"
  },
  "engines": {
    "npm": ">=8.7.0"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.10.0",
    "@grpc/proto-loader": "^0.7.10",
    "baseui": "^14.0.0",
    "lodash": "^4.17.21",
    "next": "14.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "styletron-engine-monolithic": "^1.0.0",
    "styletron-react": "^6.1.1"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.202",
    "@types/node": "^20.11.17",
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.19",
    "eslint": "^8.56.0",
    "eslint-config-next": "14.1.0",
    "eslint-plugin-baseui": "^13.0.0",
    "typescript": "^5.3.3"
  },
  "overrides": {
    "d3-color": "3.1.0",
    "react-window": "1.8.7",
    "react-uid": "2.3.2",
    "react-map-gl": "5.3.19",
    "react-virtualized-auto-sizer": "1.0.22"
  }
}